    "@types/jest": "^29.5.11",
    "@types/mapbox-gl": "^2.7.19",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mapbox-gl": "^3.0.1",
    "rollup": "^4.9.5",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
